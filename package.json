{
  "name": "contador-de-bebidas",
  "version": "1.0.0",
  "description": "Aplicativo para contar e gerenciar o consumo de bebidas",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --mode production",
    "build:dev": "npm run clean && webpack --mode development",
    "start": "webpack serve --mode development",
    "start:hot": "webpack serve --hot --mode development",
    "analyze": "webpack --mode production --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "optimize": "webpack --mode production --progress"
  },
  "keywords": [
    "bebidas",
    "contador",
    "gerenciamento"
  ],
  "author": "Seu Nome",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.26.0",
    "@types/tailwindcss": "^3.0.11",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^8.4.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.11.0",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "dotenv": "^16.4.7",
    "dotenv-webpack": "^8.1.0",
    "glob": "^11.0.1",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.1",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^6.7.0",
    "purgecss-webpack-plugin": "^7.0.2",
    "rimraf": "^5.0.5",
    "style-loader": "^3.2.1",
    "tailwindcss": "^2.2.19",
    "terser-webpack-plugin": "^5.3.11",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "chart.js": "^3.9.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-zoom": "^2.2.0",
    "date-fns": "^4.1.0",
    "firebase": "^9.23.0",
    "core-js": "^3.36.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
